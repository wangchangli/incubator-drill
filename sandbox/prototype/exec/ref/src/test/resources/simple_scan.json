{
  head:{
    type:"apache_drill_logical_plan",
    version:"1",
    generator:{
      type:"manual",
      info:"na"
    }
  },
  storage:[
    {
      type:"console",
      name:"console"
    },
    {
      type:"fs",
      name:"fs1",
      root:"file:///"
    },
    {
      type:"classpath",
      name:"cp"
    }
  ],
  query:[
    {
      op:"sequence",
      do:[
	    {
	      op: "scan",
	      memo: "initial_scan",
	      ref: "donuts",
	      storageengine: "cp",
	      selection: {
	        path: "/donuts.json",
	        type: "JSON"
	      }
	    },
        {
          op: "store",
          memo: "output sink",
          storageengine: "console",
          target: {pipe: "STD_OUT"}
        }
       ]
     }
   ]
 }